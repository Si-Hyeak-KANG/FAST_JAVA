package ch15;

public class Customer implements Buy, Sell{

	@Override
	public void sell() {
		
		System.out.println("Customer sell");
	}

	@Override
	public void buy() {
		System.out.println("Customer buy");
	}


	
	public void hello() {
		System.out.println("hello");
	}

	@Override
	public void order() {
		// TODO Auto-generated method stub
		Buy.super.order();
		Sell.super.order();
	}

}
